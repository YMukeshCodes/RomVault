{
  "version": 2,
  "outputDirectory": "dist/public",
  "functions": {
    "api/**/*.ts": {
      "includeFiles": "data/**,dist/data/**,api/storage.ts,articles/**,roms/**,public/articles/**,public/roms/**"
    }
  },
  "rewrites": [
    {
      "source": "/roms/level-up-your-fps-game-why-korean-gamers-trust-mart-hack-for-premium-gaming-support-services",
      "destination": "/roms/level-up-your-fps-game-why-korean-gamers-trust-mart-hack-for-premium-gaming-support-services.html"
    },
    {
      "source": "/roms/unleashing-gaming-dominance-with-engineowning-the-go-to-platform-for-undetected-multiplayer-game-cheats",
      "destination": "/roms/unleashing-gaming-dominance-with-engineowning-the-go-to-platform-for-undetected-multiplayer-game-cheats.html"
    },
    {
      "source": "/roms/gba-roms/(.*)",
      "destination": "/api/redirect"
    },
    {
      "source": "/roms/3ds-roms/(.*)",
      "destination": "/api/redirect"
    },
    {
      "source": "/roms/gamecube-roms/(.*)",
      "destination": "/api/redirect"
    },
    {
      "source": "/articles/(.*)",
      "destination": "/api/redirect"
    },
    {
      "source": "/roms/([^/]+)/([^/]+)$",
      "destination": "/api/roms/$1/$2"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}